{
	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"serverfarmsName": {
			"type": "string"
		},
		"siteLocation": {
			"type": "string"
		},
		"fazProjectName": {
			"type": "string"
		},
		"repoUrl": {
			"type": "string"
		},
		"branch": {
			"type": "string",
			"defaultValue": "master"
		}
	},
	"variables": {},
	"resources": [
		{
			"comments": "Generated by FUNCY AZURE 0.2.0",
			"type": "Microsoft.Web/serverfarms",
			"sku": {
				"name": "Y1",
				"tier": "Dynamic",
				"size": "Y1",
				"family": "Y",
				"capacity": 0
			},
			"kind": "functionapp",
			"name": "[parameters('serverfarmsName')]",
			"apiVersion": "2015-08-01",
			"location": "[parameters('siteLocation')]",
			"properties": {
				"name": "[parameters('serverfarmsName')]",
				"numberOfWorkers": 0
			},
			"dependsOn": []
		},
		{
			"comments": "Generated by FUNCY AZURE 0.2.0",
			"type": "Microsoft.Web/sites",
			"kind": "functionapp",
			"name": "[parameters('fazProjectName')]",
			"apiVersion": "2015-08-01",
			"location": "[parameters('siteLocation')]",
			"properties": {
				"name": "[parameters('fazProjectName')]",
				"hostNames": [
					"[concat(parameters('fazProjectName'),'.azurewebsites.net')]"
				],
				"enabledHostNames": [
					"[concat(parameters('fazProjectName'),'.azurewebsites.net')]",
					"[concat(parameters('fazProjectName'),'.scm.azurewebsites.net')]"
				],
				"hostNameSslStates": [
					{
						"name": "[concat(parameters('fazProjectName'),'.azurewebsites.net')]",
						"sslState": 0,
						"thumbprint": null,
						"ipBasedSslState": 0
					},
					{
						"name": "[concat(parameters('fazProjectName'),'.scm.azurewebsites.net')]",
						"sslState": 0,
						"thumbprint": null,
						"ipBasedSslState": 0
					}
				],
				"serverFarmId": "[resourceId('Microsoft.Web/serverfarms', parameters('serverfarmsName'))]",
				"containerSize": 128
			},
			"dependsOn": [
				"[resourceId('Microsoft.Web/serverfarms', parameters('serverfarmsName'))]"
			],
			"resources": [
				{
					"apiVersion": "2015-08-01",
					"name": "appsettings",
					"type": "config",
					"dependsOn": [
						"[resourceId('Microsoft.Web/Sites', parameters('fazProjectName'))]"
					],
					"properties": {
						"AppSettingKey1": "Some value",
						"AppSettingKey2": "My second setting",
						"AppSettingKey3": "My third setting",
						"AppSettingKey4": "My fourth setting"
					}
				},
				{
					"apiVersion": "2015-08-01",
					"name": "web",
					"type": "sourcecontrols",
					"dependsOn": [
						"[resourceId('Microsoft.Web/Sites', parameters('fazProjectName'))]"
					],
					"properties": {
						"RepoUrl":"[parameters('repoUrl')]",
						"branch": "[parameters('branch')]",
						"IsManualIntegration": true 
					}
				}
			]
		}
	]
}